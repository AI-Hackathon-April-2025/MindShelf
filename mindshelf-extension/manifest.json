{
    "name": "MindShelf",
    "description": "Extension to store MindFiles",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icons/icon.png"
    },
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}